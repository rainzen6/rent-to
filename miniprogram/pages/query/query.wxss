
.searchbar{
  width: 100%;
  height: 100rpx;
  background:#9AECDB ;
  display: flex;
  align-items: center;
  justify-content: center;
  
  
}
.insearch{
  display: flex;
  width: 100%;
  background-color: white;
  position: relative;
  width: 90%;
  height: 70rpx;
  border-radius: 50rpx;
  align-items: center;


}
.input{
  position: relative;
  width: 600rpx;
  height: auto;
  text-align:center;
  
}
.icon-sousuo{
  
  position: relative;
  align-items:flex-start;
  font-size: 50rpx;

}
.page-body {
  display: flex;
  height: 100%;
  width: 95%;
  align-items: center;
  border-radius: 30rpx;
  margin: 10rpx;
 background-color: rgb(238, 238, 238);
}
.item {
  display: flex;
  justify-content: space-between;
  padding: 20rpx;
  width: 95%;
  height: 200rpx;
  border-bottom: 1rpx solid #eee;
  
  

}

.leftbar{
  background-color: #fff;
