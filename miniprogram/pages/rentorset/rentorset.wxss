.bigbox{
  display: flex;
  align-items: center;
  justify-content: space-around;

}
.functional{
  display: flex;
  
  align-items: center;
  width: 150%;
}
.rentorname{
  
  width: 80%;
  display: flex;
  border-radius: 20rpx;
  border: 4rpx solid rgb(39, 38, 38);
   align-items: center;
   
}

.rentoricon{
  display: flex;
  align-items: center;
  justify-content: center;
   font-size: 50rpx;
   width: 80rpx;
   height: 80rpx;
   color: rgb(238, 107, 56);
}
.main_namebar{
  
  width: 400rpx;
  font-size: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-left: 4rpx solid #000;
}


.room_id{
  
  width: 80%;
  display: flex;
  border-radius: 20rpx;
  border: 4rpx solid rgb(39, 38, 38);
   align-items: center;
   
}

.roomicon{
  display: flex;
  align-items: center;
  justify-content: center;
   font-size: 50rpx;
   width: 80rpx;
   height: 80rpx;
  color: rgb(189, 221, 113);
}
.main_roombar{
  
  width: 400rpx;
  font-size: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-left: 4rpx solid #000;
}
.select_name_room{
  display: flex;
  flex-direction: column;
  
  width: 80%;
  height: 200rpx;
  align-items: center;
  justify-content: space-around;
}

.addiconbox{
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100rpx;
  height: 100rpx;
  border-radius: 20rpx;
  border: 4rpx solid rgb(192, 190, 190);
}
.icon-xinzengzuke_icon{
  color: #31b1d1;
  font-size: 80rpx;
  
}

.page-body{
  display: flex;
  flex-direction: column;
  
  height: 100rpx;

  background-color: rgb(86, 118, 221);
  border-radius: 15rpx;
  
  margin: 5rpx;
  margin-bottom: 5rpx;
}

.item{
  display: flex;
  height: 100%;
  justify-content: space-between;
}

.info{
  display: flex;
  height: 100%;
  align-items: center;
  color: white;
  font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
  
}

.name{
  margin-left: 40rpx;

}

.room{
  margin-left: 40rpx;
}

.button{
  display: flex;
  align-items: center;
  
  height: 50%;
  position: relative;
  top: 25rpx;
}

.update{
  margin-right: 30rpx;
  color: white;
  border-radius: 10rpx;
  
}

.delete{
  margin-right: 20rpx;
  color: white;
  border-radius: 10rpx;
  
  
}

.Modalbox{
  
  width: 80%;
  height: 40%;
  background-color: #fff;
  opacity: 1;
  position: fixed;
  top: 300rpx;
  left: 75rpx;
  border: 4rpx solid #000;
  border-radius: 20rpx;
}

.Modal{
  display: flex;
  flex-direction: column;

}
.titlebox{
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #fff;
  width: 100%;
  height: 100rpx;
}
.title{
  
  font-size: 40rpx;
}

.select_name_room_tj{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-evenly;
    
    height: 250rpx;
}
.addbutton{
  
  display: flex;
  height: 100rpx;
  align-items: center;
  justify-content: space-around;
  
}
.icon-cuowu{
  font-size: 60rpx;
  color: rgb(236, 49, 49);
}
.icon-dui{
  font-size: 60rpx;
  font-weight: 20rpx;
  color: rgb(39, 223, 49);
}
.updatebutton{
  
  display: flex;
  height: 100rpx;
  align-items: center;
  justify-content: space-around;
}

