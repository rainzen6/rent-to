.all{
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: #f2f7f5;
}
.header{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  
  height: 400rpx;
  border-radius: 0 0 60rpx 60rpx;
  background: #9AECDB;
}
.Loginbtn{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.nicheng{
  font-size: 48rpx;
  font-weight: 400;
  color: white;
}
.login_out{
  font-size: 30rpx;
  color: gray;
  margin-top: 20rpx;
}
.root{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.iconfont{
  
  height: 200rpx;
  width: 200rpx;
  border-radius: 50%;
  
}
.touxiang{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  
  background: white;

}
.item{
  
  margin: 6rpx 2rpx 2rpx 2rpx;
  padding: 20rpx;
  background:white;
